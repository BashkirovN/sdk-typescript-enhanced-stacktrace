{
  "extends": "@tsconfig/node14/tsconfig.json",
  "version": "4.2.2",
  "compilerOptions": {
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,
    "incremental": true,
    "rootDir": ".",
    "outDir": "../../lib/worker",
    "paths": {
      "@interfaces": [
        "../interfaces"
      ],
      "@interfaces/*": [
        "../interfaces/*"
      ]
    }
  },
  "include": [
    "./**/*.ts"
  ],
  "references": [
    {
      "path": "../interfaces/tsconfig.json"
    },
    {
      "path": "../activities/tsconfig.json"
    },
    {
      "path": "../workflows/tsconfig.json"
    }
  ]
}
